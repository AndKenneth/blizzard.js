{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [
      "index.js",
      "./lib/blizzard.js",
      "./lib/endpoints.js",
      "./lib/account.js",
      "./lib/d3.js",
      "./lib/sc2.js",
      "./lib/wow.js"
    ]
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "readme": "./README.md",
    "template": "./node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
